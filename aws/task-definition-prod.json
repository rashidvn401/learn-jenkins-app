{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "determined-wolf-LearnJenkins-pipeline-demo",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "386606043713.dkr.ecr.eu-north-1.amazonaws.com/myjenkinsapp:1.0.96",
            "portMappings": [{
               "name": "nginx-80-tcp",
               "containerPort": 80,
               "hostPort": 80,
               "protocol": "tcp",
               "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::386606043713:role/ecsTaskExecutionRole"
}